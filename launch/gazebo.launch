<launch>
  <!-- Load robot description from Xacro -->
  <param name="robot_description" command="$(find xacro)/xacro $(find my_robot_description)/urdf/my_robot.xacro"/>

  <!-- Spawn the robot in Gazebo -->
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"
        args="-param robot_description -urdf -model my_robot" />

  <!-- Start an empty Gazebo world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch"/>
</launch>

